{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "AppDataConnection": "Data Source=localhost;Initial Catalog=IdentityAppData;Integrated Security=True",
    "IdentityConnection": "Data Source=localhost;Initial Catalog=IdentityAppUserData;Integrated Security=True"
  },
  "BearerTokens": {
    "ExpiryMins": "1440",
    "Key": "mySuperSecretKey"
  }
}
